{
    "name": "snicco/fortress-bundle",
    "license": "commercial",
    "description": "An authentication and security bundle for snicco/kernel",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "snicco/event-dispatcher": "^1.2.0",
        "snicco/http-routing": "^1.2.0",
        "snicco/http-routing-bundle": "^1.2.0",
        "snicco/application-layer-bundle": "dev-master",
        "snicco/signed-url": "^1.2.0",
        "snicco/encryption-bundle": "^1.2.0",
        "snicco/better-wpdb": "^1.2.0",
        "snicco/testable-clock": "^1.2.0",
        "snicco/content-negotiation-middleware" : "^1.2.0",
        "snicco/payload-middleware" : "^1.2.0",
        "webmozart/assert": "^1.10.0",
        "pragmarx/google2fa": "^8.0.0",
        "paragonie/password_lock": "^3.1.0",
        "wp-cli/wp-config-transformer": "v1.3.0",
        "bjeavons/zxcvbn-php": "^1.3"
    },
    "require-dev": {
        "nyholm/psr7": "^1.5.0",
        "snicco/event-dispatcher-testing": "^1.2.0",
        "snicco/testing-bundle": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Enterprise\\Bundle\\Fortress\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Enterprise\\Bundle\\Fortress\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
